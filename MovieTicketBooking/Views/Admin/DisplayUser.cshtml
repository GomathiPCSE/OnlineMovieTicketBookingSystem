@model IEnumerable<MovieTicketBooking.Models.SignUpModel>
@{
    ViewBag.Title = "User Details";
    Layout = "~/Views/Shared/_Master.cshtml";
}
<h2 class="headingMovie">User Details</h2>
<div class="adminContent">

    <table cellspacing="15">
        <tr>
            <th>First Name </th>
            <th>Last Name </th>
            <th> Mobile Number</th>
            <th>MailId</th>
            <th>Password</th>
        </tr>
        @foreach (MovieTicketBooking.Models.SignUpModel user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.MobileNumber</td>
                <td>@user.MailId</td>
                <td>@user.Password</td>
                <td class="Submit">
                    @Html.ActionLink("Delete", "DeleteUser", new { id = user.UserId },

    new { onclick = "return confirm('Are sure wants to delete?');" })
                </td>
            </tr>
        }
    </table>
</div>



